{
    "manifest_version": 3,
    "name": "IDMChrome",
    "short_name": "IDMC",
    "description": "Simple Internet Download Manager Extension", 
    "version": "1.0",
    "background":{
        "service_worker":"/background.js"
    },
    "permissions": ["downloads", "storage", "activeTab", "downloads.shelf", "notifications"],
    "action": {  
     "default_popup": "/popup.html"
    }
}